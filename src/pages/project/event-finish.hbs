{{set
  'page'
}}
{{#>layout}}
<div class="coinus">
  <div class="l-wrap">
    {{>header}}
    <div class="coinus-box">
      <h2 class="coinus-box-title">프로젝트 현황</h2>
      <div class="l-coinus">
        <div class="coinus-event">
          <div class="coinus-event-result">현재 진행중인 프로젝트는 <em class="pink">10</em>개 입니다.</div>
          <div class="coinus-event-tab">
            <a href="#"><span>진행예정 프로젝트</span></a>
            <a href="event.html"><span>진행중인 프로젝트</span></a>
            <a href="#" class="is-active"><span>완료된 프로젝트</span></a>
          </div>
          <ul id="event-run" class="coinus-list coinus-list--event is-edit">
            <li>
              <div class="cell"><span>Project 1</span></div>
              <div class="cell">
                <a href="market.html" class="btn btn--green">수정하기</a>
                <a href="market.html" class="btn btn--red">삭제하기</a>
              </div>
            </li>
          </ul>
          {{>pagination}}
        </div>
      </div>
    </div>
    {{>footer}}
  </div>
</div>
{{/layout}}
{{#*inline 'script'}}{{/inline}}